{
  "name": "lab",
  "description": "Test utility",
  "version": "10.9.0",
  "repository": "git://github.com/hapijs/lab",
  "main": "lib/index.js",
  "keywords": [
    "test"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "bossy": "3.x.x",
    "diff": "2.x.x",
    "eslint": "3.0.x",
    "eslint-config-hapi": "9.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "espree": "3.x.x",
    "find-rc": "3.0.x",
    "handlebars": "4.x.x",
    "hoek": "4.x.x",
    "items": "2.x.x",
    "json-stable-stringify": "1.x.x",
    "json-stringify-safe": "5.x.x",
    "mkdirp": "0.5.x",
    "source-map-support": "0.4.x"
  },
  "devDependencies": {
    "code": "3.x.x",
    "cpr": "1.1.x",
    "eslint-plugin-markdown": "1.0.0-beta.2",
    "lab-event-reporter": "1.x.x",
    "rimraf": "2.5.x"
  },
  "bin": {
    "lab": "./bin/lab"
  },
  "scripts": {
    "test": "./bin/_lab -fL -t 100 -m 3000",
    "posttest": "npm run lint-md",
    "test-cov-html": "./bin/_lab -fL -r html -m 3000 -o coverage.html",
    "lint": "./bin/_lab -d -f -L",
    "lint-md": "eslint --config hapi --rule 'strict: 0, eol-last: 0' --plugin markdown --ext md  ."
  },
  "license": "BSD-3-Clause"
}
